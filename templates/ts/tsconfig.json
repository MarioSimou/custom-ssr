    "module": "ESNext",
    "moduleResolution": "node",
    "declaration": false,
    "noEmit": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["src"]
}
